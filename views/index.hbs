				<img src="/img/mobilem4.svg" alt="m4" class="custom-m4">
			 </div>
			 <div class="col-lg-6">
				 <div class="pistol-wrapper-cart">
					<div class="pistol-cart">
						<img src="/img/pistol-white.svg" alt="pistol" class="pistol-icon">
						<p class="pistol-title"><span>DESERT EAGLE</span>Blaze</p>
						<img src="/img/deagle.svg" alt="deagle" class="main-pistol">
					</div>
					<div class="pistol-cart pistol-cart2">
						<img src="/img/pistol-white.svg" alt="pistol" class="pistol-icon">
						<p class="pistol-title"><span>MAC-10</span>Neon Rider</p>
						<img src="/img/mac.svg" alt="mac" class="main-pistol">
					</div>
					<div class="pistol-cart pistol-cart3">
						<img src="/img/pistol-white.svg" alt="pistol" class="pistol-icon">
						<p class="pistol-title"><span>GLOCK-18</span>Bullet Queen</p>
						<img src="/img/glock.svg" alt="glock" class="main-pistol">
					</div>
				</div>
			 </div>
			 <div class="col-lg-6 relative m4-desk">
				<img src="/img/m4.svg" alt="m4" class="custom-m4">
			 </div>
		 </div>
	 </div>
 </section>

 <section class="weapons">
	 <div class="container">
		 <div class="row">
			 <div class="col-lg-12">
					<div class="wrapper-flex-for-title">
					<img src="/img/offer-bottom.svg" alt="border" class="border-titile">
				 </div>
			 </div>
			<div class="custom-card-weapon-wrapper">


			{{#if user}}
						{{#each items}}
				<div class="custom-card-weapon">
					<div class="card-weapon">
						<div class="card-image center-align">
							<img class="materialboxed" width="140" src="{{this.image}}">
							<p class="card-text-price">
								<img src="/img/dollar.svg" alt="usd" class="dollar">
								{{this.price}}</p>
							<p class="black-text card-title-item">{{this.market_hash_name}}</p>
						</div>
						<div class="center space card-action">
								<button class="btn withdraw item"
								data-assetid="{{this.assetid}}" data-price="{{this.price}}">Buy</button>  
						</div>
					</div>
				</div>
			{{/each}}
			{{else}}
			{{#each items}}
				<div class="custom-card-weapon">
					<div class="card-weapon">
						<div class="card-image center-align">
							<img class="materialboxed" width="140" src="{{this.image}}">
							<p class="card-text-price">
								<img src="/img/dollar.svg" alt="usd" class="dollar">
								{{this.price}}</p>
							<p class="black-text card-title-item">{{this.market_hash_name}}</p>
						</div>
						<div class="center space card-action">
                                <a href="/auth/steam" class="btn">
                                    <img src="/img/steam.svg" alt="steam" class="steam-icon">
                                    Sign in
                                </a>
						</div>
					</div>
				</div>
			{{/each}}
			{{/if}}
			</div>

		 </div>
	 </div>
 </section>

